<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1159dip"
    android:layout_height="687dip"
    android:layout_centerInParent="true"
    android:background="@color/bg"
    android:orientation="vertical"
    tools:context=".StageOneLearnPopUpActivityFour" >

    <ImageView
        android:id="@+id/stage1_learn_popup4_bg"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:contentDescription="@string/nodesc"
        android:src="@drawable/common_question_popup" />

    <TextView
        android:id="@+id/stage1_learn_popup4_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="60dp"
        android:text="@string/stage_1_popup_4_question_1"
        android:textColor="#000000"
        android:textSize="40sp" />

    <ImageButton
        android:id="@+id/stage1_learn_popup4_next_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="1000dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/next_btn"
        android:contentDescription="@string/nodesc" />

    <ImageView
        android:id="@+id/stage1_learn_popup4_mainchar"
        android:layout_width="150dp"
        android:layout_height="250dp"
        android:layout_marginTop="400dp"
        android:contentDescription="@string/nodesc"
        android:src="@drawable/mother" />

    <ImageView
        android:id="@+id/stage1_learn_popup4_mirror"
        android:layout_width="400dp"
        android:layout_height="400dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="250dp"
        android:contentDescription="@string/nodesc"
        android:src="@drawable/mirror" />

    <ImageView
        android:id="@+id/stage1_learn_popup4_mirror_material"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginLeft="320dp"
        android:layout_marginTop="470dp"
        android:contentDescription="@string/nodesc"
        android:src="@drawable/learn3_1_m" />

    <RelativeLayout
        android:id="@+id/stage1_learn_popup4_blank_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="570dp"
        android:layout_marginTop="470dp"
        android:background="@color/bg"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/stage1_learn_popup4_answer_layout1"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/stage1_learn_popup4_image_material6"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:contentDescription="@string/nodesc"
                android:src="@drawable/learn3_1" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/stage1_learn_popup4_image_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="800dp"
        android:layout_marginTop="200dp"
        android:background="@color/bg"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/stage1_learn_popup4_image_layout1"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/stage1_learn_popup4_image_material1"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:contentDescription="@string/nodesc"
                android:src="@drawable/learn3_2" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/stage1_learn_popup4_image_layout2"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_below="@id/stage1_learn_popup4_image_layout1"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/stage1_learn_popup4_image_material2"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:contentDescription="@string/nodesc"
                android:src="@drawable/learn3_3" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/stage1_learn_popup4_image_layout3"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_below="@id/stage1_learn_popup4_image_layout2"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/stage1_learn_popup4_image_material3"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:contentDescription="@string/nodesc"
                android:src="@drawable/learn3_4" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/stage1_learn_popup4_image_layout4"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignBottom="@id/stage1_learn_popup4_image_layout3"
            android:layout_toRightOf="@id/stage1_learn_popup4_image_layout3"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/stage1_learn_popup4_image_material4"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:contentDescription="@string/nodesc"
                android:src="@drawable/learn3_5" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/stage1_learn_popup4_image_layout5"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignBottom="@id/stage1_learn_popup4_image_layout2"
            android:layout_toRightOf="@id/stage1_learn_popup4_image_layout2"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/stage1_learn_popup4_image_material5"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:contentDescription="@string/nodesc"
                android:src="@drawable/learn3_6" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/stage1_learn_popup4_image_layout6"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignBottom="@id/stage1_learn_popup4_image_layout1"
            android:layout_toRightOf="@id/stage1_learn_popup4_image_layout1"
            android:gravity="center"
            android:orientation="vertical" >

        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>